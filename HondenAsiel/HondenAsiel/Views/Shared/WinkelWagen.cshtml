@model Honden

<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="thumbnail">
        <img src="@Model" />
        <div class="caption">
            <h3 class="pull-right">@Model.Prijs.ToString("c")</h3>
            <h3>
                <a>@Model.Naam</a>
            </h3>
            <p>@Model.RasId</p>
        </div>
    </div>
    <h4>Honden naam: @Model.Naam</h4>
    <h4>Prijs van hond: @Model.Prijs.ToString("c")</h4>
</div>