@model WinkelWagenViewModel



@if (Model.WinkelWagen.WinkelWagenItems.Count > 0 )
{
    <li>
        <a asp-controller="WinkelWagen">
            <span class="glyphicon glyphicon-shopping-cart"></span>
            <span id="cart-status">
                @Model.WinkelWagen.WinkelWagenItems.Count
            </span>
        </a>
    </li>
}

